// TestArray
array($array, 10);
$array[8] = true;
say($array[8]);

// TestLambda
array($array, 3);
$array[0] = 1;
$array[1] = 2;
$array[-1] = 3;
map(lambda($x) { return $x + 1; }, $array);
say($array[0]);

// TestListen
$x;
listen($x);
say($x);

// TestWhile
$i = 0;
while ($i < 15) {
	say($i);
	$i = $i + 1;
}
if ($i == 15)
	say($i);

// TestSize
array($array, 10);
size($array, $val);
say($val);
